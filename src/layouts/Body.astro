    });

    ScrollReveal().reveal(".empresaAsociada", {
        duration: 1200,
        origin: "rigth",
        distance: "70px",
        interval: 200,
        reset: false,
    });    
    
    let objBtnGoHome = document.getElementById('btnGoHome') as HTMLDivElement;
    let vwInicial = window.innerHeight;
    let isVisible = false;
    
    // Funci칩n para manejar la visibilidad del bot칩n con throttling
    function handleScroll() {
        const posicion = window.scrollY;
        const shouldBeVisible = posicion > vwInicial - 300;
        
        if (shouldBeVisible && !isVisible) {
            objBtnGoHome?.classList.add('visible');
            isVisible = true;
        } else if (!shouldBeVisible && isVisible) {
            objBtnGoHome?.classList.remove('visible');
            isVisible = false;
        }
    }
    
    // Throttling para mejorar el rendimiento del scroll
    let ticking = false;
    document.addEventListener('scroll', () => {
        if (!ticking) {
            requestAnimationFrame(() => {
                handleScroll();
                ticking = false;
            });
            ticking = true;
        }
    });


